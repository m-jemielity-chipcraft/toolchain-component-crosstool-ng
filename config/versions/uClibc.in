    default "git" if UCLIBC_DEVEL_VCS_git
    default "svn" if UCLIBC_DEVEL_VCS_svn
    default "hg" if UCLIBC_DEVEL_VCS_hg
    default "cvs" if UCLIBC_DEVEL_VCS_cvs

config UCLIBC_DEVEL_URL
    string "Repository URL"
    default "git://git.busybox.net/uClibc"
    help
      Repository URL.

      For CVS, enter both the value of CVS root and the module name, separated
      by a space.

config UCLIBC_DEVEL_BRANCH
    string "Branch/tag to check out"
    default ""
    help
      Git/CVS: branch/tag to be checked out
      Subversion: directories to append to the repository URL (i.e. branch or tag)
      Mercurial: official guide recommends using separate repositories to maintain
      stable branches. You likely need to change the repository URL, rather than
      enter anything here.

config UCLIBC_DEVEL_REVISION
    string "Revision/changeset"
    default ""
    help
      Commit ID or revision ID to check out.
      Git: enter the commit ID to check out a commit.
      CVS: enter the date in "YYYY/MM/DD HH:MM:SS" format (UTC) to check out certain date.
      Subversion: enter the revision.

config UCLIBC_DEVEL_SUBDIR
    string "Subdirectory in the repository"
    default ""
    help
      Some projects produce releases not from the top-level directory in the
      repository, but rather from some subdirectory. If it is the case,
      specify this subdirectory here.

config UCLIBC_DEVEL_BOOTSTRAP
    string "Bootstrap command"
    default ""
    help
      Command to run after checking out. Some projects don't store the generated
      files like configure script in the repository; building out of a checked out
      working copy thus requires some extra steps. Separate multiple shell commands
      with &&.

endif

config UCLIBC_SRC_CUSTOM
    bool "Custom location"
    depends on EXPERIMENTAL
    help
      Custom directory or tarball.

if UCLIBC_SRC_CUSTOM

config UCLIBC_CUSTOM_LOCATION
    string "Custom source location"
    help
      Path to the directory or tarball with the sources.

endif

endchoice

if EXPERIMENTAL
choice
    bool "uClibc patches origin"
    default UCLIBC_PATCH_GLOBAL

config UCLIBC_PATCH_GLOBAL
    bool "Per global policy"

config UCLIBC_PATCH_BUNDLED
    bool "Bundled only"

config UCLIBC_PATCH_LOCAL
    bool "Local only"
    select PATCH_USE_LOCAL

config UCLIBC_PATCH_BUNDLED_LOCAL
    bool "Bundled, then local"
    select PATCH_USE_LOCAL

config UCLIBC_PATCH_LOCAL_BUNDLED
    bool "Local, then bundled"
    select PATCH_USE_LOCAL

config UCLIBC_PATCH_NONE
    bool "None"

endchoice
endif

config UCLIBC_PATCH_ORDER
    string
    default "bundled" if UCLIBC_PATCH_BUNDLED
    default "local" if UCLIBC_PATCH_LOCAL
    default "bundled,local" if UCLIBC_PATCH_BUNDLED_LOCAL
    default "local,bundled" if UCLIBC_PATCH_LOCAL_BUNDLED
    default "none" if UCLIBC_PATCH_NONE
    default "global"

choice
    bool "Version of uClibc"
    help
      For a released version, select the version of uClibc to download
      and build. For sources out of the vendor repository or from a custom
      location, select the version that describes those custom sources.
      Based on this version, crosstool-NG may apply some version-specific
      quirks while building uClibc.

config UCLIBC_VERY_NEW
    bool "newer than anything below"
    depends on EXPERIMENTAL
    depends on UCLIBC_SRC_DEVEL || UCLIBC_SRC_CUSTOM
    select UCLIBC_1_0_23_or_later
    select UCLIBC_later_than_1_0_23
    depends on !UCLIBC_REQUIRE_1_0_23_or_older
    depends on !UCLIBC_REQUIRE_older_than_1_0_23
    select UCLIBC_1_0_21_or_later
    select UCLIBC_later_than_1_0_21
    depends on !UCLIBC_REQUIRE_1_0_21_or_older
    depends on !UCLIBC_REQUIRE_older_than_1_0_21
    select UCLIBC_1_0_15_or_later
    select UCLIBC_later_than_1_0_15
    depends on !UCLIBC_REQUIRE_1_0_15_or_older
    depends on !UCLIBC_REQUIRE_older_than_1_0_15
    select UCLIBC_1_0_0_or_later
    select UCLIBC_later_than_1_0_0
    depends on !UCLIBC_REQUIRE_1_0_0_or_older
    depends on !UCLIBC_REQUIRE_older_than_1_0_0
    select UCLIBC_0_9_33_2_or_later
    select UCLIBC_later_than_0_9_33_2
    depends on !UCLIBC_REQUIRE_0_9_33_2_or_older
    depends on !UCLIBC_REQUIRE_older_than_0_9_33_2

config UCLIBC_V_0_9_33_2
    bool "0.9.33.2 (OBSOLETE)"
    depends on OBSOLETE
    depends on !UCLIBC_REQUIRE_later_than_1_0_23
    select UCLIBC_1_0_23_or_older
    depends on !UCLIBC_REQUIRE_1_0_23_or_later
    select UCLIBC_older_than_1_0_23
    depends on !UCLIBC_REQUIRE_later_than_1_0_21
    select UCLIBC_1_0_21_or_older
    depends on !UCLIBC_REQUIRE_1_0_21_or_later
    select UCLIBC_older_than_1_0_21
    depends on !UCLIBC_REQUIRE_later_than_1_0_15
    select UCLIBC_1_0_15_or_older
    depends on !UCLIBC_REQUIRE_1_0_15_or_later
    select UCLIBC_older_than_1_0_15
    depends on !UCLIBC_REQUIRE_later_than_1_0_0
    select UCLIBC_1_0_0_or_older
    depends on !UCLIBC_REQUIRE_1_0_0_or_later
    select UCLIBC_older_than_1_0_0
    depends on !UCLIBC_REQUIRE_older_than_0_9_33_2
    select UCLIBC_0_9_33_2_or_later
    depends on !UCLIBC_REQUIRE_later_than_0_9_33_2
    select UCLIBC_0_9_33_2_or_older

config UCLIBC_VERY_OLD
    bool "older than anything above"
    depends on OBSOLETE && EXPERIMENTAL
    depends on UCLIBC_SRC_DEVEL || UCLIBC_SRC_CUSTOM
    select UCLIBC_1_0_23_or_older
    select UCLIBC_older_than_1_0_23
    depends on !UCLIBC_REQUIRE_1_0_23_or_later
    depends on !UCLIBC_REQUIRE_later_than_1_0_23
    select UCLIBC_1_0_21_or_older
    select UCLIBC_older_than_1_0_21
    depends on !UCLIBC_REQUIRE_1_0_21_or_later
    depends on !UCLIBC_REQUIRE_later_than_1_0_21
    select UCLIBC_1_0_15_or_older
    select UCLIBC_older_than_1_0_15
    depends on !UCLIBC_REQUIRE_1_0_15_or_later
    depends on !UCLIBC_REQUIRE_later_than_1_0_15
    select UCLIBC_1_0_0_or_older
    select UCLIBC_older_than_1_0_0
    depends on !UCLIBC_REQUIRE_1_0_0_or_later
    depends on !UCLIBC_REQUIRE_later_than_1_0_0
    select UCLIBC_0_9_33_2_or_older
    select UCLIBC_older_than_0_9_33_2
    depends on !UCLIBC_REQUIRE_0_9_33_2_or_later
    depends on !UCLIBC_REQUIRE_later_than_0_9_33_2

endchoice

# A flag indicating that no valid selections exist for the version.
# This may happen if there are conflicting requirements set by
# the host system and/or other packages.
config UCLIBC_NO_VERSIONS
    bool
    select INVALID_CONFIGURATION
    default n if UCLIBC_VERY_NEW
    default n if UCLIBC_V_0_9_33_2
    default n if UCLIBC_VERY_OLD
    default y

if UCLIBC_NO_VERSIONS
comment "WARNING!"
comment "|  The uClibc package has no viable versions"
comment "|  due to conflicting constraints."
endif



config UCLIBC_VERSION
    string
    default "0.9.33.2" if UCLIBC_V_0_9_33_2
    default "new" if UCLIBC_VERY_NEW
    default "old" if UCLIBC_VERY_OLD
    default "unknown"

config UCLIBC_MIRRORS
    string
    default "http://www.uclibc.org/downloads http://www.uclibc.org/downloads/old-releases"

config UCLIBC_ARCHIVE_FILENAME
    string
    default "@{pkg_name}-@{version}"

config UCLIBC_ARCHIVE_DIRNAME
    string
    default "@{pkg_name}-@{version}"

config UCLIBC_ARCHIVE_FORMATS
    string
    default ".tar.xz .tar.bz2"

config UCLIBC_SIGNATURE_FORMAT
    string
    default ""


endif


